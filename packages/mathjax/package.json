    "web-worker": "^1.5.0",
    "xyjax": "github:sonoisa/XyJax-v3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0"
  },
  "engines": {
    "node": ">= 22.0"
  }
