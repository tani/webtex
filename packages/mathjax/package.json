{
  "name": "webtex-mathjax",
  "version": "0.16.0",
  "description": "WebTeX MathJax renderer with inline worker support",
  "type": "module",
  "main": "./dist/node/index.js",
  "browser": "./dist/browser/index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    }
